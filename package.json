{
	"name": "utech-e-commerce-backend",
	"version": "1.9.0",
	"description": "A backend for UTech e-commerce platform",
	"author": "Zimpligital (https://zimpligital.com)",
	"license": "MIT",
	"keywords": [
		"sqlite",
		"postgres",
		"typescript",
		"ecommerce",
		"headless",
		"medusa",
		"utech",
		"it",
		"gadget",
		"zimpligital",
		"backend",
		"typescript"
	],
	"scripts": {
		"build": "medusa build",
		"seed": "medusa exec ./src/scripts/seed.ts",
		"migrations": "npx medusa db:migrate",
		"sync": "npx medusa db:sync-links",
		"start": "medusa start",
		"dev": "medusa develop",
		"build-start": "medusa build && medusa start",
		"test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
		"test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
		"test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
		"test:coverage": "jest --coverage",
		"check:changed": "node ./scripts/check-changed.js",
		"format:changed": "node ./scripts/check-changed.js format",
		"lint:changed": "node ./scripts/check-changed.js lint",
		"prepare": "husky"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.883.0",
		"@aws-sdk/s3-request-presigner": "^3.883.0",
		"@medusajs/admin-sdk": "2.8.8",
		"@medusajs/cli": "2.8.8",
		"@medusajs/framework": "2.8.8",
		"@medusajs/medusa": "2.8.8",
		"@mikro-orm/core": "6.4.3",
		"@mikro-orm/knex": "6.4.3",
		"@mikro-orm/migrations": "6.4.3",
		"@mikro-orm/postgresql": "6.4.3",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "^0.57.2",
		"@opentelemetry/sdk-node": "^0.57.2",
		"@sentry/node": "9.11",
		"@sentry/opentelemetry-node": "^7.114.0",
		"@zimpligital/medusa-plugin-auth-otp": "^1.4.0",
		"@zimpligital/medusa-plugin-order-export": "^1.0.0",
		"@zimpligital/medusa-plugin-product-review": "1.8.4-alpha.0",
		"awilix": "^8.0.1",
		"chart.js": "^4.4.9",
		"class-validator": "^0.14.1",
		"csv-parse": "^5.6.0",
		"csv-stringify": "^6.5.2",
		"date-fns-tz": "^3.2.0",
		"dayjs": "^1.11.13",
		"express": "^4.17.2",
		"lodash": "^4.17.21",
		"meilisearch": "^0.41.0",
		"pdfmake": "^0.2.16",
		"pg": "8.14.1",
		"react-chartjs-2": "^5.3.0",
		"slugify": "^1.6.6",
		"vite-plugin-inspect": "^0.8.7",
		"web-push": "^3.6.7",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@jest/globals": "^29.7.0",
		"@medusajs/test-utils": "2.8.8",
		"@mikro-orm/cli": "6.4.3",
		"@stdlib/number-float64-base-normalize": "0.0.8",
		"@swc/core": "^1.12.11",
		"@swc/jest": "^0.2.39",
		"@types/express": "^4.17.13",
		"@types/jest": "^29.5.13",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/lodash": "^4.17.13",
		"@types/mime": "1.3.5",
		"@types/node": "20.17.30",
		"@types/pdfmake": "^0.2.10",
		"@types/react": "18.3.18",
		"@types/react-dom": "18.3.6",
		"@types/uuid": "^10.0.0",
		"@types/web-push": "^3.6.4",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.4.1",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-jest": "29.3.1",
		"ts-node": "^10.9.2",
		"typescript": "5.8.3",
		"vite": "5.4.17",
		"yalc": "^1.0.0-pre.53"
	},
	"engines": {
		"node": ">=20"
	}
}